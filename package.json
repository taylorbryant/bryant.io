{
  "name": "bryant.io",
  "version": "1.0.33",
  "description": "Personal website for Taylor Bryant",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "fix:import-sort": "import-sort --write '**/*.js'",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,mdx}'",
    "fix": "npm run fix:lint && npm run fix:prettier && npm run fix:import-sort"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylorbryant/bryant.io.git"
  },
  "author": "Taylor Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorbryant/bryant.io/issues"
  },
  "homepage": "https://github.com/taylorbryant/bryant.io#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "@mapbox/rehype-prism": "0.4.0",
    "@mdx-js/loader": "1.5.8",
    "@next/mdx": "9.3.4",
    "date-fns": "2.11.1",
    "dotenv": "8.2.0",
    "next": "9.3.4",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "2.7.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.1.0",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-preval": "5.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "husky": "4.2.5",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "10.1.2",
    "postcss-import": "12.0.1",
    "prettier": "2.0.4",
    "tailwindcss": "1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,mdx}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
