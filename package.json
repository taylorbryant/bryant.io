{
  "name": "taylorbryant.dev",
  "version": "0.0.1",
  "description": "Personal website for Taylor Bryant",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "fix:dependencies": "npm audit fix",
    "fix:import-sort": "import-sort --write '**/*.{js}'",
    "fix:lint": "eslint --ext .jsx --ext .js --ext .mdx . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,mdx}'",
    "fix:tailwind-css-class-order": "rustywind --write .",
    "fix": "npm run fix:dependencies && npm run fix:tailwind-css-class-order && npm run fix:import-sort && npm run fix:lint && npm run fix:prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylorbryant/taylorbryant.dev.git"
  },
  "author": "Taylor Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorbryant/taylorbryant.dev/issues"
  },
  "homepage": "https://github.com/taylorbryant/taylorbryant.dev#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@mapbox/rehype-prism": "0.5.0",
    "@mdx-js/loader": "1.6.19",
    "@next/mdx": "9.5.5",
    "date-fns": "2.16.1",
    "next": "10.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-ga": "3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-mdx": "1.8.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.1.1",
    "husky": "4.3.0",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "10.4.2",
    "prettier": "2.1.2",
    "rustywind": "0.6.7",
    "tailwindcss": "1.9.6"
  },
  "engines": {
    "node": "^12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "lint-staged": {
    "*.{js,css,md,mdx}": "npm run fix:prettier",
    "*.{js,mdx}": "npm run fix:lint",
    "*.{js}": "npm run fix:import-sort && npm run fix:tailwind-css-class-order"
  }
}
