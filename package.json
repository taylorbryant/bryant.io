{
  "name": "bryant.io",
  "version": "1.0.33",
  "description": "Personal website for Taylor Bryant",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "npm run build && next export",
    "start": "next start",
    "fix:import-sort": "import-sort --write '**/*.js'",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,mdx}'",
    "fix": "npm run fix:lint && npm run fix:prettier && npm run fix:import-sort"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylorbryant/bryant.io.git"
  },
  "author": "Taylor Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylorbryant/bryant.io/issues"
  },
  "homepage": "https://github.com/taylorbryant/bryant.io#readme",
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@apollo/react-ssr": "3.1.3",
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@mapbox/rehype-prism": "0.4.0",
    "@mdx-js/loader": "1.5.5",
    "@next/mdx": "9.2.0",
    "@types/react": "16.9.17",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link-http": "1.5.16",
    "date-fns": "2.9.0",
    "dotenv": "8.2.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "isomorphic-unfetch": "3.0.0",
    "lodash": "4.17.15",
    "next": "9.2.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "2.7.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "babel-plugin-graphql-tag": "2.5.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-preval": "4.0.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "4.0.10",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "lint-staged": "9.5.0",
    "postcss-import": "12.0.1",
    "prettier": "1.19.1",
    "tailwindcss": "1.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
